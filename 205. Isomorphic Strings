class Solution {
public:
    bool isIsomorphic(string s, string t) {
        int size1=s.length();
        int size2=t.length();
        int map1[256]={0};
        int map2[256]={0};
        if(size1!=size2){
            return false;
        }
        for(int i=0;i<size1;i++){
            if(!map1[s[i]] and !map2[t[i]]){
                map1[s[i]]=t[i];
                map2[t[i]]=s[i];
            }
            else if(map1[s[i]]!=t[i]){
                return false;
            }
        }
        return true;
    }
};
