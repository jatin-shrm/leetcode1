class Solution {
public:
    string largestOddNumber(string num) {
        string result;
        int n=num.length();
        for(int i=n-1;i>=0;i--){
            if(num[i]==49 || num[i]==51 || num[i]==53 || num[i]==55 || num[i]==57){
                string sub=num.substr(0,i+1);
                result=sub;
                break;
            }
            
        }
        return result;
    }
};
